		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/better-sqlite3": "^7.6.13",
		"better-auth": "^1.4.18",
		"better-sqlite3": "^12.6.2",
		"bits-ui": "^2.14.4",
